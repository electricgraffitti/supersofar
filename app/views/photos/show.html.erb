<% page_title(@photo.title)%>

<%= render @photo %>
<div class="clear"></div>
<div class="full_wrap">
<% if current_admin %>
<%= link_to 'Edit Photo', edit_photo_path(@photo) %> |
<%= link_to 'Delete Photo', @photo, :confirm => 'Are you sure?', :method => :delete %> |
<% end %>
<%= link_to 'Back To Album', photo_album_path(@photo.photo_album) %>
</div>