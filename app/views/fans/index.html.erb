<h1>Listing fans</h1>

<table>
  <tr>
    <th>Email</th>
    <th>Password</th>
    <th>Password salt</th>
    <th>Persistence token</th>
  </tr>

<% @fans.each do |fan| %>
  <tr>
    <td><%=h fan.email %></td>
    <td><%=h fan.password %></td>
    <td><%=h fan.password_salt %></td>
    <td><%=h fan.persistence_token %></td>
    <td><%= link_to 'Show', fan %></td>
    <td><%= link_to 'Edit', edit_fan_path(fan) %></td>
    <td><%= link_to 'Destroy', fan, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New fan', new_fan_path %>