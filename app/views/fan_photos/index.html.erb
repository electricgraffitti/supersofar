<h1>Listing fan_photos</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Description</th>
    <th>Fan photo album</th>
    <th>Permalink</th>
  </tr>

<% @fan_photos.each do |fan_photo| %>
  <tr>
    <td><%=h fan_photo.title %></td>
    <td><%=h fan_photo.description %></td>
    <td><%=h fan_photo.fan_photo_album_id %></td>
    <td><%=h fan_photo.permalink %></td>
    <td><%= link_to 'Show', fan_photo %></td>
    <td><%= link_to 'Edit', edit_fan_photo_path(fan_photo) %></td>
    <td><%= link_to 'Destroy', fan_photo, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New fan_photo', new_fan_photo_path %>