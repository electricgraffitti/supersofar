<% page_title(@fan_photo.title)%>
<div class="header">
  <h2><%= (@fan_photo.title) %></h2>
</div>
<%= render @fan_photo %>
<div class="clear"></div>
<div class="grid_10">
  <%= render :partial => "comments/comment_block", :locals => {:model_object => @fan_photo} %>
</div>
<div class="clear"></div>
<div class="full_wrap">
<% if current_fan %>
<%= link_to 'Edit Photo', edit_photo_path(@fan_photo) %> |
<%= link_to 'Delete Photo', @fan_photo, :confirm => 'Are you sure?', :method => :delete %> |
<% end %>
<%= link_to 'Back To Album', fan_photo_album_path(@fan_photo.fan_photo_album) %>
</div>